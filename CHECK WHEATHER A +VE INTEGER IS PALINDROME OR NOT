// check whether a number is palindrome or  not

#include <stdio.h>

int main() {
   int a,b,c,s=0;
   printf("Enter a number: ");
   scanf("%d",&c);
   a=c;
   
   
   while(a>=1){
       b=a%10;
       a=a/10;
       s=s*10+b;
   }
   
   
   
   if(s==c){
       printf("\nEntered number is A Palindrome Number");
   }
   else{
       printf("\nEntered number is NOT a Palindrome Number");
   }
       
   return 0;
}
