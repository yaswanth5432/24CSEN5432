//program to print the sum of digits in +ve integer number


#include <stdio.h>

int main() {
   int a,b,c,sum=0;
   printf("enter any number: ");
   scanf("%d",&c);
   a=c;
   printf("\n\n");
   printf(" the sum of the digits in the entered number is : ");
   
   while(a>=1){
       b=a%10;
       a=a/10;
       
       sum=sum+b;
   }
   printf("%d",sum);
   
   
   
   return 0;
}
