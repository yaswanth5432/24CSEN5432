// print the sum of digits 
#include <stdio.h>

int main() {
   int a,b,c,s=0;
   printf("enter a number: ");
   scanf("%d",&c);
   a=c;
   printf("\n\n");
   printf(" the sum of the enetred digits is : ");
   
   while(a>=1){
       b=a%10;
       a=a/10;
       
       s+=b;
   }
   printf("%d",s);
   
   
   
   return 0;
}
