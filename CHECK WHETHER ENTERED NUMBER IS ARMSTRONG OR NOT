// check whether the entered number is armstrong or not
#include <stdio.h>
int main() {
   int a,b,c,s=0;
   printf("Enter a number: ");
   scanf("%d",&c);
   a=c;
   while(a>=1){
       b=a%10;
       a=a/10;
       s=s+b*b*b;
   }
   
   if(s==c){
       printf("\n\nThe entered number IS an Armstrong Number");
   }
   else{
       printf("\n\nthe entered number is NOT an Armstrong Number");
   }
   
   return 0;
}
